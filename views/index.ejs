                        e.preventDefault();
                        var data = form.serialize();
                        $.ajax({
                            url: `mailto:support@acefinanceonline.com`,
                            method: 'post',
                            data: data,
                            success: function (response) {
                                if (response.success) {
                                    form.find('input[name=email]').val('');
                                    notify('success', response.message);
                                }else{
                                    notify('error', response.error);
                                }
                            }
                        });
                    });
        
                })(jQuery);
